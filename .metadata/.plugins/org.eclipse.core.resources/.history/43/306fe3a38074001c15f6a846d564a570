package Arrays;

public class Array2D {

	public static void main(String[] args) {
		
		int [] [] array = new int [2] [3];
		
		array[0][0] = 1;
		array[0][1] = 1;
		array[0][2] = 1;
		
		array[1][0] = 1;
		array[1][1] = 1;
		array[1][2] = 1;
		
		for (int i=0; i<array.length; i++) {
			
			for (int j=0; j<array[i].length; j++) {
				
				System.out.println(array[i][j]);
			}
		}
	}

}
